{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@app/enums": [
        "src/app/enums"
      ],
      "@app/database-models": [
        "src/app/models/database-models"
      ],
      "@app/custom-models": [
        "src/app/models/custom-models"
      ],
      "@app/models": [
        "src/app/models/extended-models"
      ],
     
      "@app/grid-custom-templates": [
        "src/custom-templates/grid"
      ]
    },
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}